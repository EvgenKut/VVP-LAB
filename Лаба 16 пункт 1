using System;
public class Program
{
	public static void Main(string[] args)
	{
    Console.WriteLine("Введите целое положительное число N");
    int N = Convert.ToInt32(Console.ReadLine());
    if (N>0){
        int[] a= new int[N];
        int i=1;
        for(int b=0;b<N;b++){
            a[b]=i;
            i+=2;
        }
        for(int j=0;j<N;j++)
        Console.Write(a[j]+" ");
    }else Console.WriteLine("Введенное число не соответствует условию.");
	}
}
