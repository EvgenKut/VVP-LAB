using System;
public class Program
{
	public static void Main(string[] args)
	{
    Console.WriteLine("Введите целое положительное число A");
    int A = Convert.ToInt32(Console.ReadLine());
    Console.WriteLine("Введите целое положительное число B (B должно быть меньше A)");
    int B = Convert.ToInt32(Console.ReadLine());
    if (A>0 & B>0 & A>B){
        int x=B;
        while(A-x>=B)
        x+=B;
    Console.WriteLine("Незанятая часть отрезка A составляет "+(A-x));
    }else
    Console.WriteLine("Введенные числа не соответствуют условиям.");
	}
}
