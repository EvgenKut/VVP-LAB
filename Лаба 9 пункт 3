using System;
public class Program
{
	public static void Main(string[] args)
	{
    Console.WriteLine("Введите номер K-го дня года:");
    int K = Convert.ToInt32(Console.ReadLine());
    Console.WriteLine("Введите номер N дня недели 1 января:");
    int N = Convert.ToInt32(Console.ReadLine());
    int d = (N+K - 1)%7;
    Console.WriteLine("Номер дня недели  "+ (K) + "-го дня года: "+ d);
	}
}
