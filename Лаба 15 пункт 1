using System;
public class Program
{
    public static double PowerA3 (double A){
    double B;
    B=A*A*A;
    return B;
    }
	public static void Main(string[] args){
    Console.WriteLine("Введите пять вещественных чисел.");
    double a = Convert.ToDouble(Console.ReadLine());
    double b = Convert.ToDouble(Console.ReadLine());
    double c = Convert.ToDouble(Console.ReadLine());
    double d = Convert.ToDouble(Console.ReadLine());
    double e = Convert.ToDouble(Console.ReadLine());
    Console.WriteLine("Третья степень каждого числа соответственно равна "+(PowerA3(a))+"; "+(PowerA3(b))+"; "+(PowerA3(c))+"; "+(PowerA3(d))+"; "+(PowerA3(e)));
	}
}
