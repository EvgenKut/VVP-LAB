using System;
					
public class Program
{
	public static void Main(string[] args)
	{
	Console.WriteLine("Введите координаты точки А (x1,y1)\n");
	double x1 = Convert.ToDouble(Console.ReadLine());
	double y1 = Convert.ToDouble(Console.ReadLine());
	Console.WriteLine("Введите координаты точки B (x2,y2)\n");
	double x2 = Convert.ToDouble(Console.ReadLine());
	double y2 = Convert.ToDouble(Console.ReadLine());
	double s = Math.Sqrt(Math.Pow((x2-x1),2)+Math.Pow((y2-y1),2));
	Console.WriteLine("Расстояние между точками A и B равно\n");
	Console.WriteLine(s);
	}
}
